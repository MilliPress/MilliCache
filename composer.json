{
    "name": "millipress/millicache",
    "description": "Redis Full Page Cache for WordPress",
    "type": "wordpress-plugin",
    "license": "GPL3",
    "authors": [
        {
            "name": "Philipp Wellmer",
            "email": "hello@milli.press"
        }
    ],
    "require": {
        "php": ">=7.4",
        "predis/predis": "^2.2"
    },
    "require-dev": {
        "wp-coding-standards/wpcs": "^3.0",
        "phpcompatibility/phpcompatibility-wp": "*",
        "php-stubs/wp-cli-stubs": "^2.10",
        "phpstan/phpstan": "@stable",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "szepeviktor/phpstan-wordpress": "^1.3",
        "swissspidy/phpstan-no-private": "^0.2.0",
        "roave/security-advisories": "dev-latest",
        "coenjacobs/mozart": "^0.7.1"
    },
    "suggest": {
        "ext-zlib": "Allows compressing the cache with zlib."
    },
    "scripts": {
        "phpstan": "phpstan analyse -c phpstan.neon --memory-limit 1G",
        "lint": "phpcs --standard=phpcs.xml -s",
        "fix": "phpcbf --standard=phpcs.xml",
        "post-install-cmd": [
            "\"vendor/bin/mozart\" compose",
            "composer dump-autoload"
        ],
        "post-update-cmd": [
            "\"vendor/bin/mozart\" compose",
            "composer dump-autoload"
        ]
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "mozart": {
            "dep_namespace": "MilliCache\\",
            "dep_directory": "/src/",
            "classmap_directory": "/classes/",
            "classmap_prefix": "MC_"
        }
    }
}
